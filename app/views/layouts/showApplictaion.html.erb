<!DOCTYPE html>
<html class="h-full">
  <head>
    <title>FiveTrial</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_pack_tag 'application', media: 'all'%>
    <%= javascript_pack_tag 'application'%>
    <%= javascript_pack_tag 'showApplication'%>
  </head>

  <body class="h-full relative">
    <div class="h-full flex flex-col ">
      <div>
        <%= render "shared/navbar" %>
      </div>

      <div @click.stop="onclick" id="controllerShow" class="flex flex-grow bg-teal-200 overflow-x-scroll overflow-y-hidden">
        <%= yield %>
      </div>
    </div>
  </body>
</html>
